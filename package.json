{
  "name": "typewiz-classifier",
  "version": "1.0.0",
  "description": "a typescript interface predictor",
  "main": "index.js",
  "author": "Pavel 'PK' Kaminsky <kaminsky.pavel@gmail.com> (http://www.pavel-kaminsky.com)",
  "license": "ISC",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev src/importJsonToMongo.ts",
    "format": "prettier --write src/**/*.{json,ts}",
    "lint": "tslint --project .",
    "test": "yarn lint",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --debug-brk --inspect ./node_modules/jest/bin/jest.js --runInBand",
    "precommit": "lint-staged"
  },
  "homepage": "www.pavel-kaminsky.com",
  "dependencies": {
    "@phenomnomnominal/tsquery": "^2.0.0-beta.4",
    "@types/progress": "^2.0.1",
    "@types/stemmer": "^1.0.1",
    "chalk": "^2.4.1",
    "ejs": "^2.6.1",
    "es6-promise": "^4.2.4",
    "husky": "^1.0.0-rc.6",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.1",
    "mongoose": "^5.1.5",
    "numeral": "^2.0.6",
    "progress": "^2.0.0",
    "stemmer": "^1.0.2",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/ejs": "^2.5.1",
    "@types/fetch-mock": "^6.0.1",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^23.1.0",
    "@types/lodash": "^4.14.108",
    "@types/mkdirp": "^0.5.2",
    "@types/mongodb": "^3.0.20",
    "@types/mongoose": "^5.0.16",
    "@types/node": "~9.6.2",
    "@types/node-fetch": "^1.6.9",
    "@types/numeral": "^0.0.22",
    "fetch-mock": "^6.4.2",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1",
    "ts-jest": "^22.4.6",
    "ts-node-dev": "^1.0.0-pre.25",
    "tslint": "^5.10.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn format && yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.ts": [
      "prettier --parser typescript --write",
      "tslint --fix",
      "git add"
    ]
  }
}
