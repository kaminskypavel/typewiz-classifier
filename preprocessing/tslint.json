{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "quotemark": false,
    "one-variable-per-declaration": false,
    "member-ordering": false,
    "no-trailing-whitespace": false,
    "eofline": false,
    "indent": false,
    "no-unused-variable": [
      true
    ],
    "max-line-length": [
      200
    ],
    "one-line": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "trailing-comma": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "src/**/*.spec.{ts,tsx}",
      "./jest-setup.ts"
    ]
  }
}
